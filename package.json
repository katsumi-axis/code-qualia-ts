{
  "name": "code-qualia-ts",
  "version": "1.1.0",
  "description": "A tool for communicating developer intuition and code quality perception to AI through configurable parameters. Supports Jest, Vitest, NYC, and C8 coverage.",
  "main": "dist/index.js",
  "bin": {
    "code-qualia-ts": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:integration": "jest --testPathPattern=integration",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "code-quality",
    "developer-tools",
    "typescript",
    "ai-tools",
    "code-analysis",
    "jest",
    "vitest",
    "coverage",
    "testing"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/code-qualia-ts.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/code-qualia-ts/issues"
  },
  "homepage": "https://github.com/yourusername/code-qualia-ts#readme",
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.20",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@typescript-eslint/typescript-estree": "^7.0.2",
    "cli-table3": "^0.6.3",
    "commander": "^12.0.0",
    "csv-writer": "^1.6.0",
    "glob": "^10.3.10",
    "minimatch": "^9.0.3",
    "simple-git": "^3.22.0",
    "yaml": "^2.3.4"
  }
}